{
  "name": "@lyslabs.ai/lys-flash",
  "version": "1.8.1",
  "description": "High-performance TypeScript client for Solana transaction execution - Ultra-low latency trading on Solana with Pump.fun, AMM, and SPL Token support",
  "keywords": [
    "solana",
    "blockchain",
    "trading",
    "trading-bot",
    "pump.fun",
    "pump-fun",
    "meteora",
    "dbc",
    "dynamic-bonding-curve",
    "damm",
    "damm-v1",
    "damm-v2",
    "cp-amm",
    "dynamic-amm",
    "dlmm",
    "raydium",
    "raydium-clmm",
    "raydium-cpmm",
    "raydium-ammv4",
    "raydium-launchpad",
    "zeromq",
    "zmq",
    "high-performance",
    "low-latency",
    "typescript",
    "execution-engine",
    "transaction",
    "defi",
    "crypto",
    "solana-trading",
    "mev",
    "durable-nonce",
    "multi-broadcast",
    "lys-flash",
    "lyslabs",
    "amm",
    "spl-token"
  ],
  "homepage": "https://github.com/lyslabs-ai/lys-flash#readme",
  "bugs": {
    "url": "https://github.com/lyslabs-ai/lys-flash/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyslabs-ai/lys-flash.git"
  },
  "license": "MIT",
  "author": "LYS Labs <hello@lyslabs.ai> (https://lyslabs.ai)",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "test": "tests"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:performance": "vitest run tests/performance",
    "test:coverage": "vitest run --coverage",
    "docs": "typedoc src/index.ts --out docs-generated",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "dependencies": {
    "zeromq": "^6.0.0",
    "msgpackr": "^1.11.0"
  },
  "devDependencies": {
    "@meteora-ag/cp-amm-sdk": "^1.0.0",
    "@meteora-ag/dlmm": "^1.0.0",
    "@meteora-ag/dynamic-amm-sdk": "^1.0.0",
    "@meteora-ag/dynamic-bonding-curve-sdk": "^1.0.0",
    "@raydium-io/raydium-sdk-v2": "^0.2.32-alpha",
    "@solana/web3.js": "^1.95.0",
    "@types/bn.js": "^5.1.5",
    "@types/node": "^20.0.0",
    "bn.js": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "tweetnacl": "^1.0.3",
    "typedoc": "^0.25.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.95.0",
    "tweetnacl": "^1.0.3",
    "@meteora-ag/dynamic-bonding-curve-sdk": "^1.0.0",
    "@meteora-ag/dynamic-amm-sdk": "^1.0.0",
    "@meteora-ag/cp-amm-sdk": "^1.0.0",
    "@meteora-ag/dlmm": "^1.0.0",
    "@raydium-io/raydium-sdk-v2": "^0.2.32-alpha",
    "bn.js": "^5.2.0"
  },
  "peerDependenciesMeta": {
    "@meteora-ag/dynamic-bonding-curve-sdk": {
      "optional": true
    },
    "@meteora-ag/dynamic-amm-sdk": {
      "optional": true
    },
    "@meteora-ag/cp-amm-sdk": {
      "optional": true
    },
    "@meteora-ag/dlmm": {
      "optional": true
    },
    "@raydium-io/raydium-sdk-v2": {
      "optional": true
    },
    "bn.js": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "module": "./dist/index.mjs"
}
